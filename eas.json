{
  "cli": {
    "version": ">= 5.9.2"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "staging": {
      "android": {
        "buildType": "apk",
        "credentialsSource": "local"
      },
      "channel": "staging"
    },
    "production": {}
  },
  "submit": {
    "production": {}
  }
}
